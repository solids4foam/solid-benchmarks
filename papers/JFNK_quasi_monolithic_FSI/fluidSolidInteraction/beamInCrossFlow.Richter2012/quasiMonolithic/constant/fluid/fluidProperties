FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fluidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fluidModel    newtonIcoFluid;

newtonIcoFluidCoeffs
{
    fluidFluxExtrapolationAlgorithm1 yes;
    addDivPhiUDamping no;
    pressureScaleFactor 1.0;

    stabilisation
    {
        type RhieChow;
        scaleFactor 1.0;

        // type laplacian;
        // omega omega [0 -2 1 0 0 0 0] 1.0;
    }

    optionsFile    petscOptions.Up; // used?
}

