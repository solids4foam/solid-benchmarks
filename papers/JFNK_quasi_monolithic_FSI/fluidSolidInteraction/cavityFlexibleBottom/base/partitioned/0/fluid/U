FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        #include "velocityInletProfile"
    }

    walls
    {
        // Can be used in openfoam but not available in foam extend
        //type            noSlip;

        // Can be used in all versions
        type              fixedValue;
        value             uniform (0 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    interface
    {
        type            newMovingWallVelocity;
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
