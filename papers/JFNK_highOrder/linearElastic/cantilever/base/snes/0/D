FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        //type            fixedDisplacement;
        //value    uniform (0 0 0);
        type            analyticalCantileverDisplacement;
        P 0.1e6;
        E 200e9;
        nu 0.3;
        L 2;
        D 0.1;
        value        uniform (0 0 0);
    }
    right
    {
        //type            solidTraction;
        //traction        uniform (0 -1e6 0);
        //pressure        uniform 0;
        type             analyticalCantileverTraction;
        P 0.1e6;
        E 200e9;
        nu 0.3;
        L 2;
        D 0.1;
    }
    top
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 0;
    }
    bottom
    {
        type            solidTraction;
        traction        uniform (0 0 0);
        pressure        uniform 0;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


