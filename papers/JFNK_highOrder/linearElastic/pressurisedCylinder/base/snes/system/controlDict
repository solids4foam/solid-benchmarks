FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libanalyticaPressurisedCylinder.so");

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    pressurisedCylinderAnalytical
    {
        type analyticalPressurisedCylinder;
        p 10e6;
        E 1e10;
        nu 0.3;
        Ro 18.625;
        Ri 7;
        cellDisplacement yes;
        cellStress yes;
        pointStress no;
        pointDisplacement no;
    }
    transformStressToCylindrical
    {
        type        transformStressToCylindrical;

        origin      (0 0 0);
        axis        (0 0 1);
    }
}

// ************************************************************************* //
