FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    patch0
    {
        type            manufacturedSolution;
        E               200e9;
        nu              0.3;
        ax              1e-3;
        ay              1e-3;
        az              1e-3;
        value           uniform (0 0 0);
    }
}


