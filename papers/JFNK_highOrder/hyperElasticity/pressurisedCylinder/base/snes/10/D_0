FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "10";
    object      D_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            empty;
    }
    symmetry-y
    {
        type            symmetry;
    }
    outer
    {
        type            solidTraction;
        nonOrthogonalCorrections true;
        useUndeformedArea false;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        secondOrder     false;
        extrapolateValue false;
        setEffectiveTraction false;
        relaxationFactor 1;
        value           uniform (0 0 0);
        gradient        uniform (0 0 0);
    }
    symmetry-x
    {
        type            symmetry;
    }
    inner
    {
        type            solidTraction;
        nonOrthogonalCorrections true;
        useUndeformedArea false;
        traction        uniform (0 0 0);
        pressureSeries  
{
        file            "$FOAM_CASE/constant/timeVsPressure";
        outOfBounds     clamp;
}
        secondOrder     false;
        extrapolateValue false;
        setEffectiveTraction false;
        relaxationFactor 1;
        value           uniform (0 0 0);
        gradient        uniform (0 0 0);
    }
    front
    {
        type            empty;
    }
}


