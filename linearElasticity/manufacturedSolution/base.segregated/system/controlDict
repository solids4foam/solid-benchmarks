FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libmanufacturedSolution.so");

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    MMSCubeAnalytical
    {
        type manufacturedSolution;
        E    200e9;
        nu   0.3;
        ax   2;
        ay   4;
	az   6;
        cellDisplacement yes;
        pointDisplacement yes;
	cellStress yes;
	pointStress yes;
    }
}

