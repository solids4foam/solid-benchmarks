FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "1";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    negX
    {
        type            symmetry;
    }
    negY
    {
        type            symmetry;
    }
    posZ
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    posY
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    negZ
    {
        type            symmetry;
    }
    cavity
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    posX
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


